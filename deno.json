{
  "tasks": {
    "start": "deno run --allow-net='0.0.0.0' --allow-all --watch=static/,routes/ dev.ts"
  },
  "importMap": "./import_map.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "options": {
    "useTabs": true,
    "lineWidth": 160,
    "indentWidth": 2,
    "singleQuote": true,
    "proseWrap": "preserve"
  }
}
